echo "running as:"
whoami

echo 'alias jupyter=/opt/conda/bin/jupyter' >> /home/jovyan/.bashrc

# These are not available by the time the jupyter server starts, use BEDROCK_ACCESS_KEY, BEDROCK_SECRET_KEY instead
