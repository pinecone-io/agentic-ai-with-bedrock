cd /

echo "cloning the workshop repo..."
git clone https://github.com/pinecone-io/agentic-ai-with-bedrock

echo "unzipping workshop data..."
cd /agentic-ai-with-bedrock/data/
unzip agentic_ai_with_bedrock_dataset.zip

